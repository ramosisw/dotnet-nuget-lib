<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>$id$</id>
    <version>$version$</version>
    <title>Template NuGet Lib</title>
    <authors>Julio Ramos</authors>
    <owners>ramos-isw</owners>
    <description>NuGet Lib</description>
    <summary>** summary</summary>
    <releaseNotes>** release notes</releaseNotes>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <language>en-US</language>
    <license type="file">LICENSE</license>
    <tags>dotnet-new templates</tags>
    <packageTypes>
      <packageType name="Template" />
    </packageTypes>
    <repository type="git" url="https://github.com/nuget/template.git" />
    <dependencies>
      <group targetFramework=".NETStandard2.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="LICENSE" target="LICENSE" />
    <file src="templates\**\*" exclude="templates\**\bin\**;templates\**\obj\**" target="content" />
    <file src="*.nuget" target="content\Nuget.nuspec" />
  </files>
</package>